{
  "Process": {
    "prefix": "process",
    "body": ["process ${1:ProcessName} {", "\t$0", "}"]
  },
  "Task": {
    "prefix": "task",
    "body": ["task ${1:TaskName}"]
  },
  "User Task": {
    "prefix": "user",
    "body": ["user ${1:UserTask}"]
  },
  "Service Task": {
    "prefix": "service", 
    "body": ["service ${1:ServiceTask}"]
  },
  "Start Event": {
    "prefix": "start",
    "body": ["start "]
  },
  "End Event": {
    "prefix": "end",
    "body": ["end "]
  },
  "Gateway XOR": {
    "prefix": "xor",
    "body": ["xor ${1:Decision} {", "\t$0", "}"]
  },
  "Gateway AND": {
    "prefix": "and",
    "body": ["and ${1:Parallel} {", "\t$0", "}"]
  },
  "Subprocess": {
    "prefix": "subprocess",
    "body": ["subprocess ${1:Subprocess} {", "\t$0", "}"]
  },
  "Pool": {
    "prefix": "pool",
    "body": ["pool ${1:PoolName} {", "\t$0", "}"]
  },
  "Lane": {
    "prefix": "lane", 
    "body": ["lane ${1:LaneName} {", "\t$0", "}"]
  },
  "Flow": {
    "prefix": "->",
    "body": ["${1:from} -> ${2:to}"]
  }
}